public class WareHouses {
    private static int idNumberWareHause = 1;
    private int clonedIdnumber;
    private String codiname;
    private double variebleOfStock;
    private String nameProduct;
    private int quantitieOfStock;

    private Products product;

    public WareHouses(int clonedIdnumber, String nameProduct, int quantitieOfStock) {
        this.clonedIdnumber = clonedIdnumber;
        nameProduct = nameProduct;
        this.quantitieOfStock = quantitieOfStock;
    }

    public WareHouses(String codiname) {
        this.idNumberWareHause++;
        this.codiname = codiname;
    }

    public int getQuantitieOfStock() {
        return quantitieOfStock;
    }

    public void setQuantitieOfStock(int quantitieOfStock) {
        this.quantitieOfStock = quantitieOfStock;
    }

    public String getNameProduct() {
        return nameProduct;
    }

    public void setNameProduct(String nameProduct) {
        this.nameProduct = nameProduct;
    }

    public double getVariebleOfStock() {
        return variebleOfStock;
    }

    public void setVariebleOfStock(double variebleOfStock) {
        this.variebleOfStock = variebleOfStock;
    }

    public String getCodiname() {
        return codiname;
    }

    public void setCodiname(String codiname) {
        this.codiname = codiname;
    }

    public int getClonedIdnumber() {
        return clonedIdnumber;
    }

    public void setClonedIdnumber(int clonedIdnumber) {
        this.clonedIdnumber = clonedIdnumber;
    }

    public static int getIdNumberWareHause() {
        return idNumberWareHause;
    }

    public static void setIdNumberWareHause(int idNumberWareHause) {
        WareHouses.idNumberWareHause = idNumberWareHause;
    }
}